import org.evrete.benchmarks.models.misc.TypeA;
import org.evrete.benchmarks.models.misc.TypeB;
import org.evrete.benchmarks.models.misc.TypeC;
import org.evrete.benchmarks.models.misc.TypeD;
import org.evrete.benchmarks.Drools2Rhs

rule "Rule 1" salience 20 activation-group "ag1"
  when
      $a : TypeA($a.i > 5)
  then
      $a.i = $a.i - 1;
      update($a)
      Drools2Rhs.out("Rule 1");
end

rule "Rule 2" salience 100 activation-group "ag1"
  when
      $a : TypeA($a.i > 5)
  then
      $a.i = $a.i - 1;
      update($a)
      Drools2Rhs.out("Rule 2");
end

